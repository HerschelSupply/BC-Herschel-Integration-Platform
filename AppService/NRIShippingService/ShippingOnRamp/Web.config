<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <namespace name="BC.Integration" />

    <!--alias alias="<Name to use in register node below>" 
               type="<Name of class including namespace>, 
									    <Dll assembly filename>, 
                      Version=1.0.0.0, 
                      Culture=neutral, 
									    PublicKeyToken=<Signing Cert ID. will be 'dcedabc31c4978dc' for HerschelKey cert>,
									    processorArchitecture=MSIL"/-->

    <alias alias="IInstrumentation" type="BC.Integration.Interfaces.IInstrumentation, BC.Integration.Interfaces, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>
    <alias alias="InstrumentationDB" type="BC.Integration.Utility.InstrumentationDB, BC.Integration.Utility.InstrumentationDB, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>

    <alias alias="IConfiguration" type="BC.Integration.Interfaces.IConfiguration, BC.Integration.Interfaces, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>
    <alias alias="CentralConfiguration" type="BC.Integration.Utility.CentralConfiguration, BC.Integration.Utility.CentralConfiguration, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>

    <alias alias="IPublishService" type="BC.Integration.Interfaces.IPublishService, BC.Integration.Interfaces, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>
    <alias alias="SqsMessageBus" type="BC.Integration.Utility.SqsPublishService, BC.Integration.Utility.SqsPublishService, Version=1.0.0.0, Culture=neutral, PublicKeyToken=dcedabc31c4978dc, processorArchitecture=MSIL"/>
    <container>
      <register type="IInstrumentation" mapTo="InstrumentationDB" />
      <!--register type="IConfiguration" mapTo="LocalConfiguration" /-->
      <register type="IConfiguration" mapTo="CentralConfiguration" />
      <register type="IPublishService" mapTo="SqsMessageBus" />
    </container>
  </unity>
  <appSettings>
    <!-- Non-overridable config properties-->
    <add key="ProcessName" value="NriShippingConfirmation"/>
    <add key="ServiceId" value="NriShippingSvc"/>
    <add key="ServiceOperationId" value="NriShippingOnRamp"/>
    <add key="ServicePostOperationId" value="QueueNriShipping"/>
    <add key="CentralConfigConnString" value="Dsn=HIPMgmtAuroraDB;data source={'DRIVER={MySQL ODBC 5.3 ANSI Driver}};Trusted_Connection = true'}"/>
    <add key="TracingEnabled" value="true"/>
    <add key="TracingMessageFolderPath" value="C:\\Hip Process Files\#ProcessName#\TracingEnabled"/>
    <add key="ExceptionMessageFolderPath" value="C:\\Hip Process Files\#ProcessName#\ExceptionMsg"/>
    <add key="PickupConfirmationFolderPath" value="C:\\Hip Process Files\#ProcessName#\PickupConfirmation"/> 
    <add key="PickupCancellationFolderPath" value="C:\\Hip Process Files\#ProcessName#\PickupCancellation"/> 

    <!-- Overridable config properties-->
    <add key="MsgType" value="BC.Integration.ShippingConfirmation"/>
    <add key="Filter" value="Source|NRI API"/>
    <add key="Process" value="Type|ECommShippingConfirmation"/>

    <add key="InstrumentationConn" value="Dsn=HIPMgmtAuroraDB;data source={'DRIVER={MySQL ODBC 5.3 ANSI Driver}};Trusted_Connection = true'}"/>
    <add key="InstrumentationLevel" value="Normal"/>
    <add key="DatabaseRetryCount" value="5"/>
    <add key="DatabaseRetryInterval" value="15"/><!-- interval in seconds-->

    
    <!-- Mapping Site ID switch.  Values are 'Logic', 'HeaderClientWarehouseCode', 'DataWarehouseCode'. -->
    <add key="SiteMapping" value="Logic"/>

    <!-- Messaging queue parameters-->
    <add key="QueueUrl" value="https://sqs.us-west-2.amazonaws.com/879101183817/NriECommShippingConfirm_BC" />
    <add key="QueueUser" value="NotUsedForSqs"/>
    <add key="QueuePwd" value="NotUsedForSqs" />
    <add key="ServiceRetryCount" value="3"/>
    <add key="ServiceRetryInterval" value="15"/><!-- interval in seconds-->

    <!-- AWS SQS Credentials-->
    <add key="AWSProfileName" value="Sid.Ravalia"/>
    <add key="AWSRegion" value="us-west-2" />
    
    <!-- NRI Connectivity NRI Test -->
    <!--add key="CanConfirmUrl" value="https://canapi.nri-distribution.com:443/api/v1/orderconfirmation/confirmations"/-->
    <add key="CanConfirmUrl" value="https://apitest.nri-distribution.com/api/v1/orderconfirmation/confirmations"/>
    <add key="CanCancelUrl" value="https://apitest.nri-distribution.com/api/v1/cancelledorder/orders"/>
    <add key="CanAuthKey" value="Basic SGVyc2NoZWw6OGVmNzlhYjUtMzlhYS00NDM0LThhMWItM2EwNThiODA3MTQ2"/>  
    <add key="UsaConfirmUrl" value="https://apiustest.nri-distribution.com/api/v1/orderconfirmation/confirmations"/>
    <add key="UsaCancelUrl" value="https://apiustest.nri-distribution.com/api/v1/cancelledorder/orders"/>
    <add key="UsaAuthKey" value="Basic SGVyc2NoZWw6MDRjMzliZWEtZWYyMC00YzkxLWI1N2ItYzZlYWU5NWIyYzhm"/>
        
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true"/>
          <!-- To receive exception details in faults for debugging purposes, set the value below to true.  Set to false before deployment to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <protocolMapping>
        <add binding="basicHttpsBinding" scheme="https" />
    </protocolMapping>    
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
    <directoryBrowse enabled="true"/>
  </system.webServer>
</configuration>
